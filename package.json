		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/SerokSSR/VSCode-Hexo-Butterfly-Snippets"
    },
    "homepage": "https://snow.js.org",
    "icon": "https://cdn.jsdelivr.net/gh/SerokSSR/cdn/favicon.ico"
